{
  "agents": [
    {
      "name": "my-custom-agent",
      "agent_type": "custom",
      "config_path": "/opt/my-agent/config.json",
      "mcp_servers": [
        {
          "name": "database-server",
          "command": "npx",
          "args": ["-y", "@my-org/mcp-database-server"],
          "env": {
            "DATABASE_URL": "postgresql://...",
            "DB_PASSWORD": "***"
          },
          "transport": "stdio",
          "tools": [
            {"name": "query_database", "description": "Execute SQL queries"},
            {"name": "list_tables", "description": "List available tables"}
          ]
        },
        {
          "name": "api-server",
          "command": "uvx",
          "args": ["my-api-mcp-server"],
          "env": {
            "API_KEY": "sk-..."
          },
          "transport": "stdio",
          "packages": [
            {"name": "fastapi", "version": "0.109.0", "ecosystem": "pypi"},
            "httpx@0.27.0"
          ]
        }
      ]
    },
    {
      "name": "experimental-agent",
      "agent_type": "custom",
      "config_path": "/home/dev/my-agent/config.json",
      "mcp_servers": [
        {
          "name": "test-server",
          "command": "python",
          "args": ["-m", "my_test_server"],
          "env": {},
          "transport": "stdio"
        }
      ]
    }
  ]
}
